{
  "version": "0.1.0",
  "license": "MIT",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "type": "module",
  "files": ["dist"],
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "start": "tsdx watch",
    "build": "tsdx build",
    "test": "tsdx test",
    "lint": "tsdx lint",
    "prepare": "tsdx build",
    "size": "size-limit",
    "analyze": "size-limit --why"
  },
  "husky": {
    "hooks": {
      "pre-commit": "tsdx lint"
    }
  },
  "prettier": {
    "semi": true,
    "singleQuote": false,
    "trailingComma": "none",
    "proseWrap": "never"
  },
  "name": "arcsquared",
  "description": "Rewriting Arcthird in Typescript, which is the rewriting of Arcsecond in Coffeescript.",
  "author": "Speykious <speykious@gmail.com>",
  "module": "dist/arcsquared.esm.js",
  "repository": "https://gitlab.com/yeetsquared/arcsquared",
  "size-limit": [
    {
      "path": "dist/arcsquared.cjs.production.min.js",
      "limit": "10 KB"
    },
    {
      "path": "dist/arcsquared.esm.js",
      "limit": "10 KB"
    }
  ],
  "devDependencies": {
    "@size-limit/preset-small-lib": "^4.9.2",
    "husky": "^4.3.8",
    "size-limit": "^4.9.2",
    "tsdx": "^0.14.1",
    "tslib": "^2.1.0",
    "typescript": "^4.1.3"
  }
}

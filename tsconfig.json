{
  "include": ["src"],
  "compilerOptions": {
    "module": "commonjs",
    "target": "esnext",
    "declaration": true,
    "sourceMap": true,
    "rootDir": "./src",
    "newLine": "lf",
    "pretty": true,
    "outDir": "./lib",
    "strict": true,
    "noImplicitReturns": true,
    "moduleResolution": "node",
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
  }
}
